{
  "name": "@remix-pwa/cli",
  "version": "3.1.0",
  "description": "An elegant CLI for everything Remix PWA ðŸ’–",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/remix-pwa/monorepo.git",
    "directory": "packages/cli"
  },
  "homepage": "https://remix-pwa.run",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "remix-pwa-cli": "dist/cli.js"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "TIMING=1 eslint --fix \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "test": "vitest --run --environment node --pool=forks --poolOptions.threads.singleThread --pass-with-no-tests --config ../../vitest.config.ts"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "peerDependencies": {
    "@remix-run/dev": "^1.15.0 || ^2.0.0"
  },
  "peerDependenciesMeta": {
    "@remix-run/dev": {
      "optional": true
    }
  },
  "devDependencies": {
    "@remix-pwa/eslint-config": "^0.0.0",
    "@remix-pwa/lint-staged-config": "^0.0.0",
    "@remix-run/dev": "^2.8.1"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
