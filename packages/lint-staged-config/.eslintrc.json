{
  "extends": ["@remix-pwa/eslint-config"],
  "parser": "esprima",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "impliedStrict": true
  },
  "rules": {
    "@typescript-eslint/no-var-requires": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "root": true
}
