{
  "name": "@remix-pwa/dev",
  "version": "2.1.0-dev.7",
  "description": "An esbuild compiler for Service Workers in Remix.run",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/remix-pwa/monorepo.git",
    "directory": "packages/dev"
  },
  "homepage": "https://remix-pwa.run",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc && npm run copy",
    "prepublishOnly": "npm run build",
    "compile": "tsc && node ./build.cjs",
    "copy": "cpy cli/templates dist",
    "format": "prettier --write \"cli/**/*.ts\" \"compiler/**/*.ts\"",
    "lint": "TIMING=1 eslint --fix cli/ compiler/",
    "prepublish": "npm run build",
    "test": "vitest --run --environment node --pool=forks --poolOptions.threads.singleThread --pass-with-no-tests"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "@remix-run/dev": "^1.15.0 || ^2.0.0"
  },
  "peerDependenciesMeta": {
    "@remix-run/dev": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.23.6",
    "@babel/generator": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "arg": "^5.0.2",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.20",
    "enquirer": "^2.4.1",
    "es-module-lexer": "^1.4.1",
    "esbuild": "^0.19.3",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "magic-string": "^0.30.5",
    "ora": "^7.0.1",
    "stdin-discarder": "^0.1.0"
  },
  "devDependencies": {
    "@remix-pwa/eslint-config": "^0.0.0",
    "@remix-pwa/lint-staged-config": "^0.0.0",
    "@remix-pwa/worker-runtime": "^2.1.0-dev.1",
    "@remix-run/dev": "^2.5.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.7",
    "@types/babel__traverse": "^7.20.4",
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.202",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "vite": "^5.0.2"
  }
}
